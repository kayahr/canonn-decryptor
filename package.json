{
  "$schema": "http://json.schemastore.org/package",
  "name": "canonn-decryptor",
  "version": "1.1.2",
  "type": "module",
  "description": "Tool to decrypt the mysteries of the Elite Dangerous universe",
  "keywords": [
    "Elite: Dangerous",
    "Elite",
    "Canonn",
    "Decryptor"
  ],
  "homepage": "https://kayahr.github.io/canonn-decryptor/",
  "author": {
    "name": "Klaus Reimer",
    "email": "k@ailis.de",
    "url": "https://github.com/kayahr/"
  },
  "funding": "https://github.com/kayahr/canonn-decryptor?sponsor=1",
  "license": "MIT",
  "repository": "github:kayahr/canonn-decryptor",
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "run-s build:*",
    "build:compile": "tsc",
    "package": "vite build",
    "start": "vite",
    "test": "run-s test:*",
    "test:unit": "vitest run",
    "test:lint": "eslint --max-warnings 0",
    "test:csslint": "csslint assets",
    "test:apidoc": "typedoc --emit none",
    "test:spell": "cspell --no-progress --no-summary",
    "prepare": "run-s clean build"
  },
  "files": [
    "lib/main",
    "src/main"
  ],
  "directories": {
    "lib": "./lib/"
  },
  "devDependencies": {
    "@kayahr/eslint-config": "3.6.2",
    "@kayahr/vitest-matchers": "1.3.0",
    "@types/base64-js": "=1.5.0",
    "@types/lz-string": "=1.5.0",
    "@types/node": "=24.3.0",
    "@vitest/coverage-v8": "3.2.4",
    "cspell": "9.2.1",
    "csslint": "=1.0.5",
    "npm-run-all": "4.1.5",
    "rimraf": "6.0.1",
    "source-map-loader": "=5.0.0",
    "typedoc": "0.28.11",
    "typescript": "=5.9.2",
    "vite": "7.1.4",
    "vite-plugin-static-copy": "3.1.2",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@angular/common": "=20.2.3",
    "@angular/compiler": "^20.2.3",
    "@angular/core": "=20.2.3",
    "@angular/forms": "=20.2.3",
    "@angular/platform-browser": "=20.2.3",
    "@angular/platform-browser-dynamic": "=20.2.3",
    "@angular/router": "=20.2.3",
    "base64-js": "=1.5.1",
    "core-js": "=3.45.1",
    "lz-string": "=1.5.0",
    "rxjs": "=7.8.2",
    "start": "^5.1.0",
    "tslib": "=2.8.1",
    "zone.js": "=0.15.1"
  }
}
