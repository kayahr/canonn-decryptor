{
  "$schema": "http://json.schemastore.org/package",
  "name": "canonn-decryptor",
  "version": "0.0.1",
  "description": "Tool to decrypt the mysteries of the Elite Dangerous universe",
  "keywords": [
    "Elite: Dangerous",
    "Elite",
    "Canonn",
    "Decryptor"
  ],
  "homepage": "https://github.com/kayahr/canonn-decryptor",
  "author": {
    "name": "Klaus Reimer",
    "email": "k@ailis.de",
    "url": "https://github.com/kayahr/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kayahr/canonn-decryptor"
  },
  "scripts": {
    "clean": "del-cli lib",
    "compile": "tsc",
    "watch": "tsc -w",
    "check": "cd src && jasmine",
    "package": "webpack",
    "start": "http-server .",
    "lint": "tslint --project . && csslint assets",
    "prepare": "npm run -s clean && npm run -s compile",
    "test": "npm run -s lint && npm run -s check"
  },
  "files": [
    "lib/main",
    "src/main"
  ],
  "directories": {
    "lib": "./lib/"
  },
  "devDependencies": {
    "@types/base64-js": "^1.2.5",
    "@types/jasmine": "=2.5.52",
    "@types/lz-string": "=1.3.32",
    "copy-webpack-plugin": "^4.0.1",
    "csslint": "^1.0.5",
    "del-cli": "=1.0.0",
    "http-server": "^0.10.0",
    "jasmine": "=2.6.0",
    "systemjs": "=0.20.14",
    "tslint": "=5.4.3",
    "typescript": "=2.3.4",
    "webpack": "=2.6.1"
  },
  "dependencies": {
    "@angular/common": "=4.2.3",
    "@angular/compiler": "=4.2.3",
    "@angular/core": "=4.2.3",
    "@angular/forms": "=4.2.3",
    "@angular/platform-browser": "=4.2.3",
    "@angular/platform-browser-dynamic": "=4.2.3",
    "@angular/router": "=4.2.3",
    "base64-js": "^1.2.1",
    "core-js": "=2.4.1",
    "lz-string": "=1.4.4",
    "rxjs": "=5.4.1",
    "tslib": "=1.7.1",
    "zone.js": "=0.8.12"
  }
}
